generator client {
  provider = "prisma-client-js"
  output   = "./../generated/client"
}

datasource db {
  provider = "mysql"
}

model Account {
  id         Int      @id @default(autoincrement())
  name       String
  balance    Decimal  @default(0) @db.Decimal(18, 2)
  created_at DateTime @default(now())
  active     Boolean  @default(true)
}
